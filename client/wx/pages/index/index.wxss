.background {
  width: 750rpx;
  height: 100%;
  background: linear-gradient(#615c59 30%, #f1f1f1 70%);
  padding-top: 60rpx;
}

.icon {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}

.header {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 20rpx;
}

.title {
  font-size: 40rpx;
  color: #e5e0da;
  margin-left: 30rpx;
}

.message {
  display: flex;
  flex-direction: column;
  font-size: 25rpx;
  color: #d9d2ca;
  padding: 20rpx;
}

.location {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-bottom: 20rpx;
}

.between {
  margin-right: 20rpx;
}

.location-icon {
  width: 25rpx;
  height: 25rpx;
}

.ticket {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  margin-top: 30rpx;
}

.ticketBox-1 {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx;
  background: #fff;
  width: 690rpx;
  height: 60rpx;
  border-radius: 10rpx;
}

.ticketBox-2 {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  background: #EEEEEE;
  padding: 20rpx;
  width: 690rpx;
  height: 60rpx;
  border-radius: 10rpx;
  margin-top: 20rpx;
}

.ticket-icon {
  width: 30rpx;
  height: 30rpx;
  margin-right: 20rpx;
}

.ticket-left {
  display: flex;
  flex-direction: row;
}

.ticket-right {
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 20rpx;
  color: #cfcfcf;
}

.right-icon {
  width: 20rpx;
  height: 20rpx;
}

.picture {
  display: flex;
  flex-direction: column;
  margin-top: 20rpx;
  padding: 20rpx;
  font-size: 25rpx;
  color: #c6c6c6;
}

.pictureBox {
  height: 440rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  align-content: space-around;
}

.first {
  width: 220rpx;
  height: 200rpx;
  border-radius: 20rpx;
}
